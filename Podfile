# Uncomment the next line to define a global platform for your project
# platform :ios, '10.0'

target 'Recipe' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Recipe
# Networking网络请求框架
  pod 'Moya'

# Keyboard
  pod 'IQKeyboardManagerSwift'
  
  # Auto Layout 布局
  pod 'SnapKit'
  
  # Image 下载图片
  pod 'Kingfisher'

  # JSON Mapping
  pod 'HandyJSON'
  pod 'SwiftyJSON'
  
  pod 'AliyunOSSiOS' # 阿里
  
  # Cache
  pod 'SQLite.swift'
  pod 'YYCache'
  
  # Toast
  pod 'Toast-Swift' # 吐司
  pod 'SVProgressHUD'
  
  # Pop
  pod 'STPopup' # 常用弹窗视图
  
  # UI
  pod 'ESTabBarController-swift' # Tab
  pod 'TZImagePickerController' # 图片选择
  pod 'JXPhotoBrowser' # 图片浏览器
  pod 'JXPagingView/Paging' # 多视图框架
  pod 'JXSegmentedView' # 多视图框架
  pod 'FSPagerView' # 轮播
  pod "SkeletonView" # 骨架屏
  pod 'DZNEmptyDataSet'
  pod 'MJRefresh'
  pod 'FDFullscreenPopGesture'
  
  pod 'YYText'
end
